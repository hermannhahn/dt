{
	"name": "dt",
	"version": "0.0.0",
	"description": "Tools for fast and powerful development.",
	"main": "build/dt.js",
	"bin": {
		"dt": "build/dt.js"
	},
	"scripts": {
		"eslint": "echo 'Linting...' && eslint src/**/*.ts",
		"lint-staged": "echo 'Linting...' && lint-staged",
		"prettier": "echo 'Formatting...' && prettier --write \"src/**/*.ts\"",
		"prettier:check": "echo 'Formatting...' && prettier --check \"src/**/*.ts\"",
		"dev": "nodemon --watch src --exec \"webpack --mode development --watch\" --ext *.ts,*.js,*.json && title Watching...",
		"dev:ts": "ts-node src/bin/dt.ts",
		"dev:watch": "cp src/app.config.js build/ && webpack --mode development --watch",
		"build": "webpack --mode production && cp src/app.config.js build/",
		"compile": "title Compiling... && pkg build/dt.js --target node18-win-x64,node18-macos-x64,node18-linux-x64 --out-dir ./dist && sleep 1 && cp dist/dt-win.exe bin/dt.exe && title Compiled!",
		"start": "cp src/app.config.js build/ && node build/dt.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hermannhahn/dt.git"
	},
	"keywords": [
		"openai",
		"chatgpt",
		"dev",
		"cli",
		"tools",
		"git"
	],
	"author": "Hermann Hahn",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/hermannhahn/dt/issues"
	},
	"homepage": "https://github.com/hermannhahn/dt#readme",
	"devDependencies": {
		"@babel/core": "^7.21.3",
		"@babel/eslint-parser": "^7.21.3",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.21.0",
		"@types/node": "^18.15.10",
		"babel-loader": "^9.1.2",
		"eslint": "^8.36.0",
		"eslint-plugin-import": "^2.27.5",
		"jest": "^29.5.0",
		"jest-extended": "^3.2.4",
		"lint-staged": "^13.2.0",
		"prettier": "^2.8.7",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.2",
		"webpack": "^5.76.3",
		"webpack-cli": "^5.0.1",
		"webpack-node-externals": "^3.0.0"
	},
	"dependencies": {
		"@commander-js/extra-typings": "^10.0.3",
		"commander": "^10.0.0",
		"dotenv": "^16.0.3",
		"prompts": "^2.4.2"
	}
}
